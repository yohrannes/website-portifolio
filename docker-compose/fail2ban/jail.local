[http-injection]
enabled = true
port    = http,https
filter  = http-injection
logpath = /var/log/nginx/yohrannes.com-access.log
          /var/log/nginx/grafana.yohrannes.com-access.log
maxretry = 5
bantime = 86400
findtime = 300

[sshd]
enabled = true
port    = ssh
logpath = /var/log/auth.log
backend = systemd   